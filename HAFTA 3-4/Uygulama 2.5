# importing modules
import numpy as np
import matplotlib.pyplot as plt
 
# creating dataset
data = [91,78,93,57,75,52,99,80,97,62,71,69,72,89,66,75,79,75,72,
               76,104,74,62,68,97,105,77,65,80,109,85,97,
               88,68,83,68,71,69,67,74,62,82,98,101,79,105,79,69,62,73
]
# creating class interval
classInterval = [40, 50, 60, 70, 80, 90,100,110]
 
# calculating frequency and class interval
values, base = np.histogram(data, bins=classInterval)
 
# calculating cumulative sum
cumsum = np.cumsum(values)
 
# plotting  the ogive graph
plt.plot(base[1:], cumsum, color='red', marker='o', linestyle='-')
# formatting
plt.title('Ogive Graph')
plt.xlabel('Parts Cost ($)')
plt.ylabel('Cumulative Percent Frequency')
plt.show()
